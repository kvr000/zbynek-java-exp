<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">
	<appenders>
		<console name="Console" target="SYSTEM_OUT">
			<patternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS}{UTC} [%-16t] %-5level [%logger{36}] - %msg%n"/>
		</console>
	</appenders>
	<loggers>
		<logger name="cz.znj.kvr.sw.exp.java" level="WARN" />

		<root level="debug">
			<appenderRef ref="Console" />
		</root>
	</loggers>
	<loggers>
		<logger name="io.netty.channel.socket.nio.NioServerSocketChannel" level="info" additivity="false">
			<AppenderRef ref="Console"/>
		</logger>
		<root level="info">
			<appenderRef ref="Console"/>
		</root>
	</loggers>
</configuration>
